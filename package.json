{
    "name": "davinci-coder-club-website",
    "version": "1.0.0",
    "description": "Complete website for Da-Vinci Coder Club at Assam Downtown University",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "build": "npm run minify-css && npm run minify-js",
      "minify-css": "cleancss -o public/css/style.min.css public/css/*.css",
      "minify-js": "uglifyjs public/js/*.js -o public/js/script.min.js",
      "test": "jest",
      "lint": "eslint .",
      "deploy": "docker-compose up -d",
      "backup": "mongodump --uri $MONGODB_URI --out ./backups/$(date +%Y%m%d)",
      "restore": "mongorestore --uri $MONGODB_URI --drop"
    },
    "dependencies": {
      "express": "^4.18.2",
      "mongoose": "^7.5.0",
      "cors": "^2.8.5",
      "helmet": "^7.0.0",
      "dotenv": "^16.3.1",
      "nodemailer": "^6.9.4",
      "multer": "^1.4.5-lts.1",
      "bcryptjs": "^2.4.3",
      "jsonwebtoken": "^9.0.2",
      "express-validator": "^7.0.1",
      "compression": "^1.7.4",
      "express-rate-limit": "^6.10.0",
      "morgan": "^1.10.0",
      "express-session": "^1.17.3",
      "connect-mongo": "^5.0.0"
    },
    "devDependencies": {
      "nodemon": "^3.0.1",
      "clean-css-cli": "^5.6.2",
      "uglify-js": "^3.17.4",
      "jest": "^29.6.2",
      "eslint": "^8.47.0",
      "supertest": "^6.3.3"
    },
    "keywords": ["davinci", "coder", "club", "adtu", "website", "university", "technology"],
    "author": "Da-Vinci Coder Club ADTU",
    "license": "MIT",
    "engines": {
      "node": ">=16.0.0",
      "npm": ">=8.0.0"
    }
  }